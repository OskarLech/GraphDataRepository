<UserControl x:Class="QualityGrapher.Views.QualityCheckControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QualityGrapher.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <ComboBox Name="QualityCheckComboBox" ItemsSource="{Binding SupportedQualityChecks}" Loaded="QualityCheckListBox_OnLoaded"/>
    </StackPanel>
</UserControl>
